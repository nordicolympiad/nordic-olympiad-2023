#!/usr/bin/env bash
PPATH=$(realpath ..)
USE_PARALLEL=0
. ../../../testdata_tools/gen.sh

use_solution arnar_100.py

compile random_generator.py

samplegroup
sample 1

group GA 10
for i in {01..05}; do
tc GA-$i random_generator 1 500 1 200000 1 200000 any
done;

group GB 20
for i in {01..05}; do
tc GB-$i random_generator 1 200000 1 500 1 200000 any
done;

group GC 20
for i in {01..05}; do
tc GC-$i random_generator 1 5000 1 5000 1 200000 any
done;

# TODO: Find fast way of generating this group that is reliable
group GD 20
for i in {01..05}; do
tc GD-$i random_generator 1 200000 1 200000 1 200000 one_refuse
done;

group GE 30
include_group GA
include_group GB
include_group GC
include_group GD
for i in {01..05}; do
tc GE-$i random_generator 1 200000 1 200000 1 200000 any
done;
